int pinLed = 11; 
int pinSensor = 3;
int stateSensor; 
float weight = 0;
void setup()
{
  pinMode(pinSensor, INPUT); 
  pinMode(pinLed, OUTPUT); 

}
void loop(){
  if (analogRead(pinSensor) >= 150) //esse valor Ã© determinado com testes
  {   
      stateSensor = digitalRead(pinLed);
      digitalWrite(pinLed, !stateSensor);
      delay(500);
  }
  if (weight > 0)
  {
    delay(2000);
    avgWeight = weight;
  }
}
