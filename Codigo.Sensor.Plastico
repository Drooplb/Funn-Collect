int pinoLed = 11; 
int pinoSensor = 3;
int estadoSensor; 
void setup(){
  pinMode(pinoSensor, INPUT); 
  pinMode(pinoLed, OUTPUT); 
}

void loop(){
  if (analogRead(pinoSensor) >= 150){ //esse valor Ã© determinado com testes
      estadoSensor = digitalRead(pinoLed); 
      digitalWrite(pinoLed, !estadoSensor); 
      delay(500);
  }
}
